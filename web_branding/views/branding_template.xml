<odoo>
    <data>

        <template id="fav_title" name="educat Icon" inherit_id="web.layout">
            <xpath expr="//html/head/title" position="replace">
                <title t-esc="title or 'Jhenaidah  Govt Veterinary College'"/>
            </xpath>
        </template>

        <template id="fav_ico" name="educat Icon" inherit_id="web.layout">
            <xpath expr="//html/head/link[1]" position="replace">
                <link rel="shortcut icon"
                      href='/web/binary/company_logo'
                      type="image/x-icon"/>
            </xpath>
        </template>

        <template id="fav_powered_by" name="Powered By" inherit_id="web.login_layout">
            <xpath expr='//div/div/div[2]' position="replace">
                <div class="text-center" t-if="not disable_footer">
                    <a href="http://jgvetcollegebd.com/" target="_blank">Powered by
                        <span>Jhenaidah  Govt Veterinary College</span>
                    </a>
                </div>
            </xpath>
        </template>

        <!--<template id="fav_powered_by_submenu" name="Powered By" inherit_id="web.menu_secondary">
            <xpath expr='//div[@class="o_sub_menu_footer"]' position="replace">
                <div class="o_sub_menu_footer">
                    Powered by
                    <a href="http://jgvetcollegebd.com/" target="_blank">
                        <span>Jhenaidah Govt Veterinary College</span>
                    </a>
                </div>
            </xpath>
        </template>-->

    </data>
</odoo>
